<div class="container">
  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="created-tab" data-toggle="tab" href="#created" role="tab" aria-controls="created" aria-expanded="true">Created</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="attended-tab" data-toggle="tab" href="#attended" role="tab" aria-controls="attended">Attended</a>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="created" role="tabpanel" aria-labelledby="home-tab">
        <%= render "events/show_created_events", events: current_user.created_events  %>
    </div>
    <div class="tab-pane fade" id="attended" role="tabpanel" aria-labelledby="profile-tab">
        <%= render "participated_events_partial", events: current_user.events %>
    </div>
  </div>
</div>
